#include <stdio.h>

int main() {
    int vetor[10];
    int i;

    // Preencher o vetor com 10 números
    for (i = 0; i < 10; i++) {
        printf("Informe o número %d: ", i+1);
        scanf("%d", &vetor[i]);
    }

    // Encontrar os números ímpares no vetor e imprimi-los
    printf("Números ímpares no vetor: ");
    for (i = 0; i < 10; i++) {
        if (vetor[i] % 2 != 0) {
            printf("%d ", vetor[i]);
        }
    }
    printf("\n");

    return 0;
}
